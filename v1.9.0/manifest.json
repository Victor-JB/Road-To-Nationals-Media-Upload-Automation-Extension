{
  "manifest_version": 3,
  "name": "GymACT Road2Nationals Content Uploader",
  "version": "1.9.0",
  "description": "Automatically uploads competition videos to YouTube and Road2Nationals [not affiliated].",
  "permissions": [
    "identity",
    "storage",
    "activeTab",
    "scripting",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*",
    "https://*.googleusercontent.com/*",
    "https://*.roadtonationals.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Road2Nationals Content Uploader",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}